class Solution {
    public static int searchInsert(int[] nums, int target) {
        int a=nums.length;
        for(int i=0;i<a;i++)
        {
            if(nums[i]>=target)
                return i;
        }
        return a;
    }
    public static void main(String args[])
    {
        int nums[]={1,3,5,6};
        int target=5;
        System.out.println(searchInsert(nums, target));
    }
}
