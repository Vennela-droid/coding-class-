import java.util.*;
class Palindrome
{
    public static void main(String args[])
    {
        int r,s=0,i;
        int n=343;
        i=n;
        while(n>0)
        {
            r=n%10;
            s=(s*10)+r;
            n=n/10;
        }
        if(i==s)
        {
            System.out.println("It is a palindrome number");
        }
        else
        System.out.println("It is not a palindrome number");
    }
}
