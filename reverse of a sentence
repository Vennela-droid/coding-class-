import java.util.*;
public class Reversewords
{
	public static void main(String[] args) {
	    String str;
	    Scanner sc=new Scanner(System.in);
	    System.out.print("Enter a sentence:");
	    str=sc.nextLine();
	    String s[]=str.split(" ");
        String rev="  ";
        for(int i=s.length-1;i>=0;i--)
        {
            rev+=s[i]+".";
        }
        System.out.println("Reverse string:"+rev);
    }
}
